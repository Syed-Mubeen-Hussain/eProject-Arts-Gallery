@{
    //this work is deleting the timeUp Auction Arts
    //Start

    var auction = db.Auctions.Where(x => x.auc_status == 1).ToList();
    foreach (var item in auction)
    {
        if (Convert.ToDateTime(item.auc_end_date) <= DateTime.Now)
        {
            var auction_id = db.Auctions.Where(x => x.auc_fk_art == item.Art.art_id).FirstOrDefault();
            var art_id = db.Arts.Where(x => x.art_id == item.Art.art_id).FirstOrDefault();
            auction_id.auc_status = 0;
            art_id.art_status = 0;
            db.SaveChanges();

            var bids = db.Bids.ToList();
            foreach (var bid in bids)
            {
                if (bids.Count > 0 && bid != null)
                {
                    var winAuction = db.Bids.OrderByDescending(x => x.bid_amount).FirstOrDefault();
                    var Art_Id = db.Arts.Where(x => x.art_id == winAuction.Auction.Art.art_id).FirstOrDefault();
                    if (winAuction.Customer.c_id > 0 && Art_Id.art_id > 0)
                    {
                        var data = db.Carts.Where(x => x.art_id == Art_Id.art_id && x.cus_id == winAuction.Customer.c_id).FirstOrDefault();
                        if (data == null)
                        {
                            Cart c = new Cart();
                            c.art_id = Art_Id.art_id;
                            c.cus_id = winAuction.Customer.c_id;
                            db.Carts.Add(c);
                            db.SaveChanges();
                        }
                    }
                }
            }
        }
    }

    //End
    //this work is deleting the exhibition with end date
    //start
    var exhibition = db.Exhibitions.ToList();
    if (exhibition != null && exhibition.Count > 0)
    {
        foreach (var e in exhibition)
        {
            if (Convert.ToDateTime(e.e_end_date) <= DateTime.Now)
            {
                e.e_status = 0;
                db.Entry(e).State = EntityState.Modified;
                db.SaveChanges();

                var art_exhibition = db.Art_exhibition.Where(x => x.ae_fk_e == e.e_id).ToList();
                foreach (var ArtExhibition in art_exhibition)
                {
                    ArtExhibition.ae_status = 0;
                    db.Entry(ArtExhibition).State = EntityState.Modified;
                    db.SaveChanges();
                }
            }
        }
    }
    //end
}

